<%# The below code creates an array of 10 random names %>
 <a href="/">Table of contents</a>
      
<hr>
      
<%= names = Array.new(10) { Faker::Name.unique.name  } %>
<%names.each do |a_name| %>
<ul class="list-group col-md-4 offset-md-4" width>
  <li class="d-flex justify-content-between align-items-start">
    <a href="#" class="list-group-item list-group-item-action d-flex justify-content-between align-items-center">
        <%=a_name%>
    <span class="badge text-bg-primary rounded-pill"><%= rand(0..9) %></span>
    </a>
  </li>
</ul>
<%end%>
